    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.0",
    "enzyme": "^2.9.1",
    "flow-bin": "^0.50.0",
    "gulp": "^3.9.1",
    "gulp-mocha": "^4.3.1",
    "html-webpack-plugin": "^2.29.0",
    "jsdom": "^11.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.4.2",
    "mocha-webpack": "^0.7.0",
    "nyc": "^11.0.3",
